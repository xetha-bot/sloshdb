{
	"compilerOptions": {
		"strict": true,
		"moduleResolution": "node",
		"module": "commonjs",
		"target": "es2019",
		"lib": [
			"esnext",
		],
		"sourceMap": false,
		"inlineSourceMap": false,
		"inlineSources": false,
		"outDir": "dist",
		"declaration": false,
		"removeComments": false,
		"alwaysStrict": true,
		"pretty": true,
		"noEmitOnError": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"esModuleInterop": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"dist"
	],
}