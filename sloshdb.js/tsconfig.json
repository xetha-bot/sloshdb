{
    "compilerOptions": {
        "moduleResolution": "Node",
        "module": "commonjs",
        "target": "esnext",
        "noImplicitAny": true,
        "sourceMap": false,
        "pretty": true,
        "outDir": "./dist",
        "declaration": false,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
    },
    "include": [
        "src"
    ]
}